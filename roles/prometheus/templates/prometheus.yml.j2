# https://prometheus.io

global:
  scrape_interval: 15s

scrape_configs:
  - job_name: 'prometheus'
    static_configs:
      - targets: ['localhost:9090']
    metrics_path: /prometheus/metrics

  - job_name: 'linux'
    static_configs:
      - targets:
{% for vm in groups['all'] %}
          - {{ vm }}:9100
{% endfor %}

